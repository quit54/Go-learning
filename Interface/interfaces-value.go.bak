package main

import (
	"fmt"
	"math"
)

type I interface {
	M()
}
type T struct {
	S string
}

func (t *T) M() {
	fmt.Println(t.S)
}

type F float64

func (f F) M() {
	fmt.Println(f)
}
func main() {
	var i I
	i = &T{"Hello"} //这里I作为接口，接收字符串的指针
	describe(i)
	i.M()

	i = F(math.Pi) //这里I作为接口，float64的数据类型
	describe(i)
	i.M()
}
func describe(i I) {
	fmt.Printf("(%v, %T)\n", i, i)
}
