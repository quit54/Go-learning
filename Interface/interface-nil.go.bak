package main

import "fmt"

type I interface {
	M()
}
type T struct {
	S string
}

func (t *T) M() {
	if t == nil {
		fmt.Println("<nil>")
		return
	}
	fmt.Println(t.S)
}
func main() {
	var i I

	var t *T
	i = t
	describe(i)
	i.M()
	//这两个的数据类型相同，都是接收指针作为参数
	i = &T{"hello"}
	describe(i)
	i.M()

}
func describe(i I) {
	fmt.Printf("(%v, %T)\n", i, i)
}
