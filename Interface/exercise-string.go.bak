package main

import "fmt"

type IPAddr [4]byte

//为IPAddr
func main() {
	hosts := map[string]IPAddr{ //定义一个map类型的数据
		//string是键，后面IPAddr是值
		"lookback":  {127, 0, 0, 1},
		"goodleDNS": {8, 8, 8, 8},
	}
	for name, ip := range hosts {
		fmt.Printf("%v: %v\n", name, ip) //每一次fmt包调用的时候都会查看是否符合string类型
	}
}
